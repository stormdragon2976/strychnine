startup_message off
unbind c
bind c exec /usr/bin/lxterminal
bind O exec /usr/bin/orca --replace
bind F1 exec $HOME/strychnine/shortcut-key-dialog
bind F2 exec $HOME/strychnine/run-dialog
bind f exec /usr/bin/seamonkey
bind m exec /usr/bin/mumble
exec /usr/bin/rpws init 4 -k
exec /usr/bin/nemo -n
exec /usr/bin/orca
